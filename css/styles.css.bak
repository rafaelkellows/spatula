	body > nav a{ border-radius: 0 !important; width: calc(50% - 24px); padding: 11.5px 12px; margin-bottom: 1px; opacity:1; -webkit-transition: opacity 0.3s, font-size 0.1s, width 0.1s, padding 0.1s; transition: opacity 0.3s, font-size 0.1s, width 0.1s, padding 0.1s; }
	body > nav.hide a{ border-radius: 0 !important; width: calc(33% - 23px); padding: 0; font-size: 0; margin-bottom: 1px; opacity: 0; }
	body > nav a:nth-child(2n+2){ margin-left: 1px; width: calc(50% - 26px); }
	body > nav a.fa-list-ul{ font-size: 18px; margin-left:1px; display: block; padding: 11.5px 12px; width: 6%; opacity: 1; background: #b72126; color: #fff; }
	body > nav a.active{ background: #fff; color: #b72126; }
	

	main{ max-width: calc(100% - 40px); padding: 20px 20px 0; border: 0 !important; }
	main section.features nav{ right: 6px;}
